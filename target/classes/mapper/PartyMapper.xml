<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.PartyMapper">
        <!--public Party getPartyById(@Param("partyId") Integer partyId);-->
        <select id="getPartyById"
                resultType="Party">
                SELECT * FROM party WHERE party_id = #{partyId}
        </select>
